{
  "name": "my-login-app",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "prefix": "app",
  "sourceRoot": "apps/my-login-app/src",
  "tags": [],
  "targets": {
    "build": {
      "executor": "@angular-devkit/build-angular:browser",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/apps/my-login-app",
        "index": "apps/my-login-app/src/index.html",
        "main": "apps/my-login-app/src/main.ts",
        "polyfills": ["zone.js"],
        "tsConfig": "apps/my-login-app/tsconfig.app.json",
        "inlineStyleLanguage": "scss",
        "assets": [
          {
            "glob": "**/*",
            "input": "apps/my-login-app/public"
          }
        ],
        "styles": ["apps/my-login-app/src/styles.scss"]
      },
      "configurations": {
        "production": {
          "budgets": [
            {
              "type": "initial",
              "maximumWarning": "500kb",
              "maximumError": "1mb"
            },
            {
              "type": "anyComponentStyle",
              "maximumWarning": "4kb",
              "maximumError": "8kb"
            }
          ],
          "outputHashing": "all"
        },
        "development": {
          "buildOptimizer": false,
          "optimization": false,
          "vendorChunk": true,
          "extractLicenses": false,
          "sourceMap": true,
          "namedChunks": true
        }
      },
      "defaultConfiguration": "production"
    },
    "serve": {
      "continuous": true,
      "executor": "@angular-devkit/build-angular:dev-server",
      "configurations": {
        "production": {
          "buildTarget": "my-login-app:build:production"
        },
        "development": {
          "buildTarget": "my-login-app:build:development"
        }
      },
      "defaultConfiguration": "development"
    },
    "extract-i18n": {
      "executor": "@angular-devkit/build-angular:extract-i18n",
      "options": {
        "buildTarget": "my-login-app:build"
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint"
    },
    "serve-static": {
      "continuous": true,
      "executor": "@nx/web:file-server",
      "options": {
        "buildTarget": "my-login-app:build",
        "spa": true
      }
    },
    "compile-scss": {
      "executor": "nx:run-commands",
      "options": {
        "commands": [
          "sass src/app/styles/provisioning-ui.scss apps/my-login-app/.storybook/assets/page-css-list/provisioning-ui.css --no-source-map",
          "sass src/app/styles/cloud-auto.scss apps/my-login-app/.storybook/assets/page-css-list/cloud-auto.css --no-source-map",
          "sass src/app/styles/relocation.scss apps/my-login-app/.storybook/assets/page-css-list/relocation.css --no-source-map"
        ],
        "parallel": true
      }
    },

    "storybook": {
      "executor": "@storybook/angular:start-storybook",
      "dependsOn": ["compile-scss"],
      "options": {
        "port": 6007,
        "tsConfig": "apps/my-login-app/.storybook/tsconfig.json",
        "configDir": "apps/my-login-app/.storybook",
        "browserTarget": "storybook-project:build",
        "compodoc": false,
        "styles": ["apps/my-login-app/.storybook/styles.scss"]
      },
      "configurations": {
        "ci": {
          "quiet": true
        }
      }
    },
    "build-storybook": {
      "executor": "@storybook/angular:build-storybook",
      "dependsOn": ["compile-scss"],
      "outputs": ["{options.outputDir}"],
      "options": {
        "tsConfig": "apps/my-login-app/.storybook/tsconfig.json",
        "configDir": "apps/my-login-app/.storybook",
        "browserTarget": "storybook-project:build",
        "compodoc": false,
        "styles": ["apps/my-login-app/.storybook/styles.scss"],
        "outputDir": "dist/storybook/my-login-app"
      },
      "configurations": {
        "ci": {
          "quiet": true
        }
      }
    }
  }
}
